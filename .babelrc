{
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-stage-2",
        "@babel/register",
        "stage-2"
      ],
      "plugins": [
        "transform-export-extensions",
        "transform-class-properties"
      ],
      "only": [
        "./**/*.js",
        "node_modules/jest-runtime"
      ]
    }
  }
}