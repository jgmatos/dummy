mponents/Users.js","original":{"line":38,"column":17}},{"generated":{"line":116,"column":8},"source":"components/Users.js","original":{"line":32,"column":12}},{"generated":{"line":116,"column":9},"source":"components/Users.js","original":{"line":31,"column":8}},{"generated":{"line":117,"column":0},"source":"components/Users.js","original":{"line":41,"column":5}},{"name":"Component","generated":{"line":121,"column":2},"source":"components/Users.js","original":{"line":13,"column":20}},{"name":"Users","generated":{"line":123,"column":15},"source":"components/Users.js","original":{"line":44,"column":15}}],"sources":{"components/Users.js":"import React, {Component} from 'react';\n\nconst User = ({user: {userId, id, title, body}}) => (\n    <div>\n        <hr/>\n        <p style={{margin: 20}}>User ID: {userId}</p>\n        <p style={{margin: 20}}>ID: {id}</p>\n        <p style={{margin: 20}}>Title: {title}</p>\n        <p style={{margin: 20}}>Body: {body}</p>\n    </div>\n)\n\nclass Users extends Component {\n    state = {user: {}, users: []};\n\n    onChangeHandler(e){\n        this.setState({\n            input: e.target.value,\n        })\n    }\n\n\n    fetchUsers = () => {\n        fetch('https://jsonplaceholder.typicode.com/posts')\n            .then(response => response.json())\n            .then(json => this.setState({users: json}))\n            .catch(error => alert(error.message));\n    }\n\n    render() {\n        return (\n            <div>\n                <h2>Highlighted User</h2>\n                <User user={this.state.user}/>\n                <hr/>\n                <h3>Want ten new users?</h3>\n                <button onClick={this.fetchUsers}>Click me!</button>\n                {this.state.users.map(user => (<User key={user.id} user={user}/>))}\n            </div>\n        )\n    }\n}\n\nexport default Users;\n"},"lineCount":null}},"error":null,"hash":"c973143cbec5f0e9f2686c4987b1b0e8","cacheData":{"env":{}}}